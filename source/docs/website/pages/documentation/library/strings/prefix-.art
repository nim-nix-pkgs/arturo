name: "prefix?"
type: :function
module: "Strings"
source: "https://github.com/arturo-lang/arturo/blob/master/src/library/Strings.nim#L529"
description: "check if string starts with given prefix"
returns: [
        :logical
    ]
examples: [
        {
            prefix? "hello" "he"          ; => true
            prefix? "boom" "he"           ; => false
        }
    ]
attributes: #[
    ]
arguments: #[
    string: [
            :string
        ]
    prefix: [
            :string
             :regex
        ]
    ]
related: [
        "Strings/suffix?"
         "Strings/upper?"
         "Strings/numeric?"
         "Strings/lower?"
    ]