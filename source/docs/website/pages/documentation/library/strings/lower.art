name: "lower"
type: :function
module: "Strings"
source: "https://github.com/arturo-lang/arturo/blob/master/src/library/Strings.nim#L307"
description: "convert given string to lowercase"
returns: [
        :char
         :string
         :nothing
    ]
examples: [
        {
            print lower "hello World, 你好!"      ; "hello world, 你好!"
        }
         {
            str: "hello World, 你好!"
            lower 'str                           ; str: "hello world, 你好!"
        }
         {
            ch: `A`
            lower ch    
            ; => `a`
        }
    ]
attributes: #[
    ]
arguments: #[
    string: [
            :char
             :string
             :literal
        ]
    ]
related: [
        "Strings/upper"
         "Strings/capitalize"
         "Strings/lower?"
         "Strings/render"
    ]